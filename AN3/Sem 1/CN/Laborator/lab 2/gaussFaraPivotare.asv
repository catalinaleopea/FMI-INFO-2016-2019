function [xNumeric] = gaussFaraPivotare(a, b)
    B = [A, b'];%Matricea extinsa
    %coafam matricea, ne asiguram ca nu avem elemente neutre pe linia
    %pivotului
    for i = 1:length(b)
        if B(i, i) == 0
            for j = i + 1:length(b)
                if B(j, i) == 0
                    aux = b(i, :);
                end
            end
        end
    end
end 